<p id="notice"><%= notice %></p>

<h1>Feeds</h1>

<br>
<table class="container">
  <div class="row">
    <% @feeds.each do |picture| %>
         <p><%= feed.user.name %></p>
         <p>Content <%= feed.content %></p>
         <p><%= image_tag(feed.image.url) %></p>
         <tr>
           <%= link_to 'Show', feed, data: {confirm: 'Are you sure you want to see?'}, class:'p-3' %>
           <%= link_to 'Edit', edit_feed_path(feed), data: {confirm: 'Are you sure you want to edit?'}, class:'p-3' %>
           <%= link_to 'Delete', feed, method: :delete, data: { confirm: 'Are you sure?' }, class:'p-3' %>
        </tr>
       <% end %>
       </div>
       </table>
 <br>

<button type="button" class="btn btn-info"><%= link_to 'New Feed', new_feed_path %></button>
